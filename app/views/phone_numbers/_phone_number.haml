.phone_number
  = best_in_place phone_number, :number, :ok_button => t(:ok), :cancel_button => t(:cancel)
  = link_to image_tag('multiply_blue.png', size: '24x24'), phone_number_path(phone_number), method: :delete, remote: true, id: "delete_phone_number_#{phone_number.id}", data: {confirm: 'Sure delete this phone number?'}
